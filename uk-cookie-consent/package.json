{
	"name": "termly",
	"description": "Termly WordPress Plugin",
	"version": "1.0.0",
	"scripts": {
		"start": "parcel watch ./src/css/termly.scss ./src/js/cookie-management.js ./src/js/banner-settings.js ./src/js/consent-toggle.js --public-url ../ --no-cache",
		"build": "parcel build ./src/css/termly.scss ./src/js/cookie-management.js ./src/js/banner-settings.js ./src/js/consent-toggle.js --public-url ../ --no-cache",
		"clean": "rm -rf dist/*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/resumecompanion/wordpress-plugin"
	},
	"author": "Pixel Jar",
	"license": "ISC",
	"devDependencies": {
		"@parcel/transformer-sass": "^2.9.3",
		"node-sass": "^9.0.0",
		"parcel": "^2.0.0",
		"sass": "^1.65.1"
	}
}
